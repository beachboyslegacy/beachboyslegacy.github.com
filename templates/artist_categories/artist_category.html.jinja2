<!doctype html>
<html lang="en">
    <head>
        <title>{% if artist.name == 'All' %}The Beach Boys{% else %}{{artist.name}}{% endif %} {% if category.name == 'Latest' %}Latest Releases{% else %}{{category.name}}{% endif %} - Beach Boys Legacy</title>
        <meta name="description" content="Explore The Beach Boys discography and all solo albums by Brian Wilson, Dennis Wilson, Carl Wilson, Mike Love, Al Jardine, Bruce Johnston and David Marks.">

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta http-equiv="cleartype" content="on">

        <link rel="shortcut icon" href="{{base_url}}/favicon.ico" />
        <link rel="apple-touch-icon" href="{{base_url}}/icon.png"/>
        <link rel="icon" href="{{base_url}}/icon.png"/>

        <link rel="stylesheet" type="text/css" href="{{base_url}}/css/styles.css" />

        <!-- For Facebook -->
        <meta property="og:title" content="{% if artist.name == 'All' %}The Beach Boys{% else %}{{artist.name}}{% endif %} {% if category.name == 'Latest' %}Latest Releases{% else %}{{category.name}}{% endif %} - Beach Boys Legacy"/>
        <meta property="og:description" content="Explore The Beach Boys discography and all solo albums by Brian Wilson, Dennis Wilson, Carl Wilson, Mike Love, Al Jardine, Bruce Johnston and David Marks."/>
        <meta property="og:image" content="{{base_url}}/assets/beach-boys-legacy.png">

        <!-- for Twitter -->
        <meta name="twitter:title" content="{% if artist.name == 'All' %}The Beach Boys{% else %}{{artist.name}}{% endif %} {% if category.name == 'Latest' %}Latest Releases{% else %}{{category.name}}{% endif %} - Beach Boys Legacy"/>
        <meta name="twitter:description" content="Explore The Beach Boys discography and all solo albums by Brian Wilson, Dennis Wilson, Carl Wilson, Mike Love, Al Jardine, Bruce Johnston and David Marks."/>
        <meta name="twitter:image" content="{{base_url}}/assets/beach-boys-legacy.png">


        <!-- Overlay Menu -->
        <script src="{{base_url}}/js/overlay_menu.js"></script>

        <!-- Sorting logic -->
        <script src="{{base_url}}/js/sort_items.js"></script>

        <!-- Selectors logic -->
        <script src="{{base_url}}/js/selectors.js"></script>

        <!-- Lazy image logic -->
        <script>{{partials["lazy_load.js"]}}</script>

        <!-- Lazy image logic -->
        <script>{{partials["search.js"]}}</script>

        {{partials["common_headers.html"]}}
    </head>
    <body>
        <div class="header clearfix">
            <a id="open-overlay-menu" class="header__menu-icon">â˜°</a>
            <div class="logo">
                <h1><a href="{{base_url}}">Beach Boys Legacy</a></h1>
            </div>

            <ul class="header__nav">
                <li><a class="header__nav__patreon-link" href="https://www.patreon.com/beachboyslegacy" target="_blank">Become a Patron</a></li>
                <li><a href="http://twitter.com/beachboyslegacy" target="_blank">
                  <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M24 4.557c-.883.392-1.832.656-2.828.775 1.017-.609 1.798-1.574 2.165-2.724-.951.564-2.005.974-3.127 1.195-.897-.957-2.178-1.555-3.594-1.555-3.179 0-5.515 2.966-4.797 6.045-4.091-.205-7.719-2.165-10.148-5.144-1.29 2.213-.669 5.108 1.523 6.574-.806-.026-1.566-.247-2.229-.616-.054 2.281 1.581 4.415 3.949 4.89-.693.188-1.452.232-2.224.084.626 1.956 2.444 3.379 4.6 3.419-2.07 1.623-4.678 2.348-7.29 2.04 2.179 1.397 4.768 2.212 7.548 2.212 9.142 0 14.307-7.721 13.995-14.646.962-.695 1.797-1.562 2.457-2.549z"/></svg>
                </a></li>
            </ul>

            <div class="header__search">
                <form action=".">c
                    <input list="results" type="search" id="searchBar" placeholder="Search albums, singles, books...">
                </form>
              <div class="header__search__icon">
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M23.809 21.646l-6.205-6.205c1.167-1.605 1.857-3.579 1.857-5.711 0-5.365-4.365-9.73-9.731-9.73-5.365 0-9.73 4.365-9.73 9.73 0 5.366 4.365 9.73 9.73 9.73 2.034 0 3.923-.627 5.487-1.698l6.238 6.238 2.354-2.354zm-20.955-11.916c0-3.792 3.085-6.877 6.877-6.877s6.877 3.085 6.877 6.877-3.085 6.877-6.877 6.877c-3.793 0-6.877-3.085-6.877-6.877z"/></svg>
              </div>
              <datalist id="results">
              </datalist>
            </div>

        </div>

        <div class="sidebar clearfix">

            <div class="sidebar__body">

                <div class="selector">
                    <div class="selector__title">Artist</div>
                    <div class="selector__current" id="current-artist"><span>{{artist.name}}</span>
                      <svg class="selector__icon" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M12 21l-12-18h24z"/></svg>
                    </div>
                    <ul id="artist" class="selector__options clearfix">
                        {% for other_artist in artists %}
                        <li
                            {{'class="selector__options__current"' if other_artist.name == artist.name}}
                        >
                        <a href="{{base_url}}/{{other_artist.path(category.id)}}">
                                {{other_artist.name}}
                            </a>
                        </li>
                        {% endfor %}
                    </ul>
                </div>

                <div class="selector">
                    <div class="selector__title">Category</div>
                    <div class="selector__current" id="current-category"><span>{{category.name}}</span>
                      <svg class="selector__icon" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M12 21l-12-18h24z"/></svg>
                    </div>
                    <ul id="category" class="selector__options clearfix">
                        {% for other_category in categories %}
                        <li
                        {% if other_category.name == category.name: %}
                        class="selector__options__current"
                        {% endif %}
                        {% if not other_category.items: %}
                        class="selector__options__disabled"
                        {% endif %}
                        >
                            <a
                                style="text-decoration:none"
                                href="{{base_url}}/{{other_category.path()}}"
                            >
                                {{other_category.name}}
                            </a>
                        </li>
                        {% endfor %}
                    </ul>
                </div>

                <div class="amazon-banner-wrapper">
                    <div id="amzn-assoc-ad-3d8cea96-b02c-4fae-9a98-94bdef6ac9cf"></div><script async src="//z-na.amazon-adsystem.com/widgets/onejs?MarketPlace=US&adInstanceId=3d8cea96-b02c-4fae-9a98-94bdef6ac9cf"></script>
                </div>
            </div>

            <div class="sidebar__footer">
                <a class="policy-link" href="cookies-policy.html" target="_blank">Cookies Policy</a>
                <a class="policy-link" href="privacy-policy.html" target="_blank">Privacy Policy</a>
            </div>
        </div>

        <div class="main-container">
            <div class="main-container__header clearfix">
                <ul class="sorting clearfix" id="sorting">
                    <li id="sorting-release" data-order="-1" class="sorting__current">Release Date
                      <svg class="sorting__icon" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M12 0l8 10h-16l8-10zm8 14h-16l8 10 8-10z"/></svg>
                    </li>
                </ul>

                <div class="results-number">{{category.items|length}} items</div>
            </div>

            <ul id="items" class="items">
                {% for item in items%}
                    <li
                        data-year="{{item.parent.releaseYear}}"
                        data-month="{{item.parent.releaseMonth}}"
                        data-day="{{item.parent.releaseDay}}"
                    >
                        <a class="item__header" href="{{base_url}}/item/{{item.parent.uniqueId}}.html" style="background-color:{{item.parent.backgroundColor}}" onClick="ga(\x27send\x27, \x27event\x27, \x27Details\x27, \x27click\x27, \x27Latest\x27)">
                            <img class="item__cover lazy" src="{{base_url}}/assets/preloader.svg" data-src="{{base_url}}/{{item.parent.image}}" alt="{{item.parent.name}} cover">
                        <div class="item__info">
                            <h2 class="item__title">{{item.parent.name}}</h2>
                            <h3 class="item__subtitle">{{item.parent.alternateName}}</h3>
                            <h3 class="item__year">{{item.parent.releaseYear}}</h3>
                            <div class="wpac-rating-ajax" data-wpac-chan="{{item.parent.uniqueId}}"></div>
                            <div class="item__btn">See details and editions</div>
                        </div>
                      </a>
                    </li>
                {% endfor %}
            </ul>

            {{partials["footers.html"]}}

        </div>

        {{partials["overlay_menu.html"]}}

        <script src="https://embed.widgetpack.com/widget.js" async></script>
        <script type="text/javascript">
            function wpac_ajax_init() {
                WPac.init({widget: 'Rating', id: 2265, el: 'wpac-rating-ajax'});
            }
        </script>



        <link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.css" />
        <script src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.js"></script>
        <script>
            window.addEventListener("load", function(){
                window.cookieconsent.initialise({
                    "palette": {
                        "popup": {
                            "background": "#000"
                        },
                        "button": {
                            "background": "#f1d600"
                        }
                    },
                    "content": {
                        "href": "cookies-policy.html"
                    }
                }
            )});
        </script>
    </body>
</html>


<!-- /nonessential stylesheets -->
<link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Roboto+Slab:wght@400;700&family=Roboto:ital,wght@0,300;0,400;0,700;1,300;1,400&family=Oswald&display=swapdisplay=swap" rel="stylesheet">
<!-- init rating widgets -->
<script>wpac_ajax_init();</script>
